	<queryString>
		<![CDATA[select*
   FROM( select sc.maphim ,p.tenphim, count(p.maphim) as SOVE
         from datve dv , suatchieu sc, phim p
         where dv.MASC = sc.MASC and sc.MAPHIM = p.MAPHIM and extract(month from ngaychieu) = $P{Thang}  AND extract(year from ngaychieu)= $P{Nam}
         group by sc.MAPHIM,p.tenphim
         order by count(dv.masc) desc)
 WHERE ROWNUM <= 3]]>
	</queryString>
